# vim: ft=xcompose

# currencies
<Multi_key> <dollar> <e> : "€"
<Multi_key> <dollar> <y> : "¥"
<Multi_key> <dollar> <l> : "£"
<Multi_key> <dollar> <r> : "₽"
<Multi_key> <dollar> <h> : "₴"
<Multi_key> <dollar> <d> : "₫"
<Multi_key> <dollar> <i> : "₹"
<Multi_key> <dollar> <w> : "₩"

# punctuation 
# tironian et
<Multi_key> <ampersand> <7> : "⁊"

<Multi_key> <ampersand> <ampersand> : "🙵" # fancy ampersand, needs a space after it to be wider in monospace
