{
    "default":[],
    "settings": [
        {
            "name":"Appearance Settings",
            "exec":"eww_settings.sh appearance"
        },
        {
            "name":"Input Settings",
            "exec":"eww_settings.sh input"
        },
        {
            "name":"Display Settings",
            "exec":"eww_settings.sh display"
        },
        {
            "name":"Sound Settings",
            "exec":"eww_settings.sh audio"
        },
        {
            "name":"Bluetooth Settings",
            "exec":"eww_settings.sh bluetooth"
        },
        {
            "name":"Storage Settings",
            "exec":"eww_settings.sh storage"
        },
        {
            "name":"About the System",
            "exec":"eww_settings.sh system"
        }
    ],
    "firefox":[
        {
            "name":"New Window",
            "exec":"firefox --new-window"
        },
        {
            "name":"New Private Window",
            "exec":"firefox --private-window"
        },
        {
            "name":"New Safe Mode Window",
            "exec":"firefox --safe-mode --new-instance"
        },
        {
            "name":"Youtube",
            "exec":"firefox --new-window youtube.com"
        },
        {
            "name":"Discord",
            "exec":"firefox --new-window https://discord.com/channels/@me"
        }

    ],
    "nemo":[
        {
            "name":"Filesystem Root (/)",
            "exec":"nemo --name='popup nemo' /"
        },
        {
            "name":"Computer",
            "exec":"nemo --name='popup nemo' computer://"
        },
        {
            "name":"Floating",
            "exec":"nemo --name='popup nemo'"
        },
        {
            "name":"Recently Used",
            "exec":"nemo --name='popup nemo' recent:///" 
        },
        {
            "name":"Trash",
            "exec":"nemo --name='popup nemo' trash:///" 
        }
    ],
    "office":[
        {
            "name":"Libreoffice Writer",
            "exec":"libreoffice --writer"
        },
        {
            "name":"Libreoffice Impress",
            "exec":"libreoffice --impress"
        },
        {
            "name":"Libreoffice Calc",
            "exec":"libreoffice --calc"
        },
        {
            "name":"Libreoffice Math",
            "exec":"libreoffice --math"
        },
        {
            "name":"Libreoffice Draw",
            "exec":"libreoffice --draw"
        }
    ],
    "steam":[
        {
            "name":"Steam Library",
            "exec":"env VK_ICD_FILENAMES=/usr/share/vulkan/icd.d/radeon_icd.i686.json:/usr/share/vulkan/icd.d/radeon_icd.x86_64.json steam steam://open/games"
        },
        {
            "name":"BigPicture Mode",
            "exec":"env VK_ICD_FILENAMES=/usr/share/vulkan/icd.d/radeon_icd.i686.json:/usr/share/vulkan/icd.d/radeon_icd.x86_64.json steam steam://open/bigpicture"
        },
        {
            "name":"Civilization VI",
            "exec":"gtk-launch Civ6"
        },
        {
            "name":"Civilization V",
            "exec":"gtk-launch Civ5"
        },
        {
            "name":"No Man's Sky",
            "exec":"gtk-launch nms"
        },
        {
            "name":"Celeste",
            "exec":"gtk-launch Celeste"
        }
    ],
    "screenshot":[
        {
            "name":"Screen to Disk",
            "exec":"bin/screenshot_menu.sh screen disk current noeww"
        },
        {
            "name":"Screen to Clipboard",
            "exec":"bin/screenshot_menu.sh screen clip current noeww"
        },
        {
            "name":"Selection to Disk",
            "exec":"bin/screenshot_menu.sh region disk noeww"
        },
        {
            "name":"Selection to Clipboard",
            "exec":"bin/screenshot_menu.sh region clip noeww"
        },
        {
            "name":"Open Screenshot Folder",
            "exec":"nemo --name='popup nemo' ~/Pictures/Screenshots/"
        }
    ],
    "minecraft":[
        {
            "name":"Main Instance",
            "exec":"prismlauncher --launch main"
        },
        {
            "name":"Open Instance Folder",
            "exec":"nemo --name='popup nemo' /mnt/Games/Minecraft/prismlauncher_instances/"
        },
        {
            "name":"Open Main Folder",
            "exec":"nemo --name='popup nemo' /mnt/Games/Minecraft/prismlauncher_instances/main/.minecraft/"
        },
        {
            "name":"Edit Main",
            "exec":"prismlauncher --show main"
        },
        {
            "name":"Server Console",
            "exec":"kitty --class=popup ssh dmxerv"
        }
    ]

}
