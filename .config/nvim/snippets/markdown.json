{
    "Insert code block": {
        "prefix": "codeblock",
        "body": ["```${1:language}", "$0", "```"],
        "description": "Insert fenced code block"
    },

    "Superscript":{
        "prefix":"sup",
        "body": ["<sup>${1}</sup>"],
        "description": "Create a superscript character"
    },
    "Subscript":{
        "prefix":"sub",
        "body": ["<sub>${1}</sub>"],
        "description": "Create a superscript character"
    },
    "Red text": {
        "prefix": "red",
        "body": ["<span class=r>${1:text}</span>"],
        "description": "Create red text"
    },
    "Yellow text": {
        "prefix": "yellow",
        "body": ["<span class=y>${1:text}</span>"],
        "description": "Create yellow text"
    },
    "Orange text": {
        "prefix": "orange",
        "body": ["<span class=ro>${1:text}</span>"],
        "description": "Create orange text"
    },
    "Green text": {
        "prefix": "green",
        "body": ["<span class=g>${1:text}</span>"],
        "description": "Create green text"
    },
    "Blue text": {
        "prefix": "blue",
        "body": ["<span class=b>${1:text}</span>"],
        "description": "Create blue text"
    },
    "Pink text": {
        "prefix": "pink",
        "body": ["<span class=p>${1:text}</span>"],
        "description": "Create pink text"
    },
    "Red marker": {
        "prefix": "rred",
        "body": ["<span class=rr>${1:text}</span>"],
        "description": "Create red reverse text"
    },
    "Yellow marker": {
        "prefix": "ryellow",
        "body": ["<span class=ry>${1:text}</span>"],
        "description": "Create yellow reverse text"
    },
    "Orange marker": {
        "prefix": "rorange",
        "body": ["<span class=ro>${1:text}</span>"],
        "description": "Create orange reverse text"
    },
    "Green marker": {
        "prefix": "rgreen",
        "body": ["<span class=rg>${1:text}</span>"],
        "description": "Create green reverse text"
    },
    "Blue marker": {
        "prefix": "rblue",
        "body": ["<span class=rb>${1:text}</span>"],
        "description": "Create blue reverse text"
    },
    "Pink marker": {
        "prefix": "rpink",
        "body": ["<span class=rp>${1:text}</span>"],
        "description": "Create pink reverse text"
    },
    "Heading 1":{
        "prefix":"h1",
        "body":["<h1 id=\"${2:id}\">${1:content}</h1>"]
    },
    "Heading 2":{
        "prefix":"h2",
        "body":["<h2 id=\"${2:id}\">${1:content}</h2>"]
    },
    "Heading 3":{
        "prefix":"h3",
        "body":["<h3 id=\"${2:id}\">${1:content}</h3>"]
    },
    "Heading 4":{
        "prefix":"h4",
        "body":["<h4 id=\"${2:id}\">${1:content}</h4>"]
    },
    "Heading 5":{
        "prefix":"h5",
        "body":["<h5 id=\"${2:id}\">${1:content}</h5>"]
    },
    "Link":{
        "prefix":["link", "ln"],
        "body":["[${1:text}](#${2:id})"]
    },
    "Hyperlink":{
        "prefix":["hyperlink", "url"],
        "body":["[${1:text}](${2:https://www.google.com})"]
    }

}
