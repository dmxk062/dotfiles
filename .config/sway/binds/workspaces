# vim: ft=swayconfig

bindsym {
    $mod+1 workspace number 1
    $mod+2 workspace number 2
    $mod+3 workspace number 3
    $mod+4 workspace number 4
    $mod+5 workspace number 5
    $mod+6 workspace number 6
    $mod+7 workspace number 7
    $mod+8 workspace number 8
    $mod+9 workspace number 9
    $mod+0 workspace number 10

    $mod+KP_1 workspace number 1
    $mod+KP_2 workspace number 2
    $mod+KP_3 workspace number 3
    $mod+KP_4 workspace number 4
    $mod+KP_5 workspace number 5
    $mod+KP_6 workspace number 6
    $mod+KP_7 workspace number 7
    $mod+KP_8 workspace number 8
    $mod+KP_9 workspace number 9
    $mod+KP_0 workspace number 10

    $mod+Shift+KP_1 move window to workspace number 1
    $mod+Shift+KP_2 move window to workspace number 2
    $mod+Shift+KP_3 move window to workspace number 3
    $mod+Shift+KP_4 move window to workspace number 4
    $mod+Shift+KP_5 move window to workspace number 5
    $mod+Shift+KP_6 move window to workspace number 6
    $mod+Shift+KP_7 move window to workspace number 7
    $mod+Shift+KP_8 move window to workspace number 8
    $mod+Shift+KP_9 move window to workspace number 9
    $mod+Shift+KP_0 move window to workspace number 10

    $mod+Ctrl+KP_1 move window to workspace number 1; workspace number 1
    $mod+Ctrl+KP_2 move window to workspace number 2; workspace number 2
    $mod+Ctrl+KP_3 move window to workspace number 3; workspace number 3
    $mod+Ctrl+KP_4 move window to workspace number 4; workspace number 4
    $mod+Ctrl+KP_5 move window to workspace number 5; workspace number 5
    $mod+Ctrl+KP_6 move window to workspace number 6; workspace number 6
    $mod+Ctrl+KP_7 move window to workspace number 7; workspace number 7
    $mod+Ctrl+KP_8 move window to workspace number 8; workspace number 8
    $mod+Ctrl+KP_9 move window to workspace number 9; workspace number 9
    $mod+Ctrl+KP_0 move window to workspace number 10; workspace number 10

    $mod+Grave scratchpad show
    $mod+Shift+Grave move window to scratchpad

    $mod+Shift+1 move window to workspace number 1
    $mod+Shift+2 move window to workspace number 2
    $mod+Shift+3 move window to workspace number 3
    $mod+Shift+4 move window to workspace number 4
    $mod+Shift+5 move window to workspace number 5
    $mod+Shift+6 move window to workspace number 6
    $mod+Shift+7 move window to workspace number 7
    $mod+Shift+8 move window to workspace number 8
    $mod+Shift+9 move window to workspace number 9
    $mod+Shift+0 move window to workspace number 10

    $mod+Ctrl+1 move window to workspace number 1; workspace number 1
    $mod+Ctrl+2 move window to workspace number 2; workspace number 2
    $mod+Ctrl+3 move window to workspace number 3; workspace number 3
    $mod+Ctrl+4 move window to workspace number 4; workspace number 4
    $mod+Ctrl+5 move window to workspace number 5; workspace number 5
    $mod+Ctrl+6 move window to workspace number 6; workspace number 6
    $mod+Ctrl+7 move window to workspace number 7; workspace number 7
    $mod+Ctrl+8 move window to workspace number 8; workspace number 8
    $mod+Ctrl+9 move window to workspace number 9; workspace number 9
    $mod+Ctrl+0 move window to workspace number 10; workspace number 10

    $mod+Left workspace s1
    $mod+Down workspace s2
    $mod+Right workspace s3
    $mod+Up workspace s4

    $mod+Shift+Left move window to workspace s1
    $mod+Shift+Down move window to workspace s2
    $mod+Shift+Right move window to workspace s3
    $mod+Shift+Up move window to workspace s4

    $mod+Ctrl+Left move window to workspace s1; workspace s1
    $mod+Ctrl+Down move window to workspace s2; workspace s2
    $mod+Ctrl+Right move window to workspace s3; workspace s3
    $mod+Ctrl+Up move window to workspace s4; workspace s4

    $mod+Bracketright exec $sc/in_order.sh go next
    $mod+Bracketleft  exec $sc/in_order.sh go prev
    $mod+Shift+Bracketright exec $sc/in_order.sh move next
    $mod+Shift+Bracketleft  exec $sc/in_order.sh move prev
    $mod+Ctrl+Bracketright exec $sc/in_order.sh push next
    $mod+Ctrl+Bracketleft  exec $sc/in_order.sh push prev
    $mod+Escape workspace number 1
}

mode "workspace" {
    bindsym { 
        Escape mode "default"

        1 workspace number 1
        2 workspace number 2
        3 workspace number 3
        4 workspace number 4
        5 workspace number 5
        6 workspace number 6
        7 workspace number 7
        8 workspace number 8
        9 workspace number 9
        0 workspace number 10

        Shift+1 move window to workspace number 1
        Shift+2 move window to workspace number 2

        Shift+3 move window to workspace number 3
        Shift+4 move window to workspace number 4
        Shift+5 move window to workspace number 5
        Shift+6 move window to workspace number 6
        Shift+7 move window to workspace number 7
        Shift+8 move window to workspace number 8
        Shift+9 move window to workspace number 9
        Shift+0 move window to workspace number 10

        Ctrl+3 move window to workspace number 3; workspace number 3
        Ctrl+4 move window to workspace number 4; workspace number 4
        Ctrl+5 move window to workspace number 5; workspace number 5
        Ctrl+6 move window to workspace number 6; workspace number 6
        Ctrl+7 move window to workspace number 7; workspace number 7
        Ctrl+8 move window to workspace number 8; workspace number 8
        Ctrl+9 move window to workspace number 9; workspace number 9
        Ctrl+0 move window to workspace number 10; workspace number 10

        KP_1 workspace number 1
        KP_2 workspace number 2
        KP_3 workspace number 3
        KP_4 workspace number 4
        KP_5 workspace number 5
        KP_6 workspace number 6
        KP_7 workspace number 7
        KP_8 workspace number 8
        KP_9 workspace number 9
        KP_0 workspace number 10

        Shift+KP_1 move window to workspace number 1
        Shift+KP_2 move window to workspace number 2
        Shift+KP_3 move window to workspace number 3
        Shift+KP_4 move window to workspace number 4
        Shift+KP_5 move window to workspace number 5
        Shift+KP_6 move window to workspace number 6
        Shift+KP_7 move window to workspace number 7
        Shift+KP_8 move window to workspace number 8
        Shift+KP_9 move window to workspace number 9
        Shift+KP_0 move window to workspace number 10

        Ctrl+KP_1 move window to workspace number 1; workspace number 1
        Ctrl+KP_2 move window to workspace number 2; workspace number 2
        Ctrl+KP_3 move window to workspace number 3; workspace number 3
        Ctrl+KP_4 move window to workspace number 4; workspace number 4
        Ctrl+KP_5 move window to workspace number 5; workspace number 5
        Ctrl+KP_6 move window to workspace number 6; workspace number 6
        Ctrl+KP_7 move window to workspace number 7; workspace number 7
        Ctrl+KP_8 move window to workspace number 8; workspace number 8
        Ctrl+KP_9 move window to workspace number 9; workspace number 9
        Ctrl+KP_0 move window to workspace number 10; workspace number 10



        Left workspace s1
        Down workspace s2
        Right workspace s3
        Up workspace s4

        Shift+Left move window to workspace s1
        Shift+Down move window to workspace s2
        Shift+Right move window to workspace s3
        Shift+Up move window to workspace s4

        Bracketright exec $sc/in_order.sh go next
        Bracketleft  exec $sc/in_order.sh go prev
        Shift+Bracketright exec $sc/in_order.sh move next
        Shift+Bracketleft  exec $sc/in_order.sh move prev
        Ctrl+Bracketright exec $sc/in_order.sh push next
        Ctrl+Bracketleft  exec $sc/in_order.sh push prev

        Grave scratchpad show
        Shift+Grave move window to scratchpad
        }
}

bindsym $mod+Tab mode "workspace"
